{
    "manifest_version": 3,
    "name": "Help Otrs",
    "description": "Ferramenta de alerta para o Otrs.",
    "version": "1.2.2",
    "author": "Jo√£o Felipe Lima",
    "content_scripts": [
        {
            "matches": [
                "https://suportedti.agu.gov.br/otrs/*"
            ],
            "css": [
                "src/css/style.css"
            ],
            "js": [
                "dist/bundle.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_title": "Help Otrs",
        "default_popup": "src/html/popup.html",
        "default_icon": {
            "16": "/src/assets/images/logo.png",
            "32": "/src/assets/images/logo.png",
            "48": "/src/assets/images/logo.png",
            "128": "/src/assets/images/logo.png"
        }
    },
    "icons": {
        "16": "/src/assets/images/logo.png",
        "32": "/src/assets/images/logo.png",
        "48": "/src/assets/images/logo.png",
        "128": "/src/assets/images/logo.png"
    }
}