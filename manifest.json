{
    "manifest_version": 3,
    "name": "Help Otrs",
    "description": "Ferramenta de alerta para o Otrs.",
    "version": "1.2.2",
    "author":"Jo√£o Felipe Lima",
    "content_scripts": [
        {
            "matches": ["https://suportedti.agu.gov.br/otrs/*"], 
            "css": ["style.css"],
            "js": ["index.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "action": {
        "default_title": "Help Otrs",
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/logo.png",
          "32": "/images/logo.png",
          "48": "/images/logo.png",
          "128": "/images/logo.png"
        }
    },
    "icons": {
        "16": "/images/logo.png",
        "32": "/images/logo.png",
        "48": "/images/logo.png",
        "128": "/images/logo.png"
    }
}