{
    "manifest_version": 3,
    "name": "Scarab Otrs",
    "description": "Ferramenta de alerta para o Otrs",
    "version": "0.9.9",
    "author":"Jo√£o Felipe Lima",
    "content_scripts": [
        {
            "matches": ["https://suportedti.agu.gov.br/*"],
            "css": ["style.css"],
            "js": ["index.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_title": "Scarab Otrs",
        "default_icon": {
          "16": "/images/logo.png",
          "32": "/images/logo.png",
          "48": "/images/logo.png",
          "128": "/images/logo.png"
        }
    },
    "icons": {
        "16": "/images/logo.png",
        "32": "/images/logo.png",
        "48": "/images/logo.png",
        "128": "/images/logo.png"
    }
}